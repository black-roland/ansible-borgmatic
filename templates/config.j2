# {{ ansible_managed }}
# DO NOT EDIT THIS FILE

[location]
source_directories: {{ source_directories | join(' ') }}
repository: {{ repository }}

[storage]
encryption_passphrase: {{ encryption_passphrase }}
compression: lz4

[retention]
keep_daily: 7
keep_weekly: 4
keep_monthly: 6
keep_yearly: 1

[consistency]
checks: repository archives
check_last: 1
